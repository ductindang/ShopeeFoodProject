                        </li>
                        <li class="nav-item" style="float:right">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Register">Register</a>
                        </li>
                        }
                        } *@
                        <li class="nav-item" style=" float:right">
                            <a class="login nav-link text-dark" asp-controller="Home" asp-action="Login">Login</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
