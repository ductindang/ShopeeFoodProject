@model IEnumerable<BusinessLogicLayerFront.DTOs.UserDto>

@{
    ViewData["Title"] = "Privacy Policy";
    Layout = "~/Views/Shared/_LayoutProject.cshtml";
}

<h1>Users</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FullName</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>